      },
      "express-openapi-validator": {
        "note": "Multiple dependencies use licenses",
        "allow": ["Python-2.0"]
      },
      "swagger-editor": {
        "note": "Multiple dependencies use licenses",
        "allow": ["Python-2.0","0BSD"]
      },
      "@stoplight/prism-http":{
